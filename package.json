{
  "name": "@nordicsemiconductor/package-layered-lambdas",
  "version": "0.0.0-development",
  "description": "Packages lambda functions and layers for AWS lambda",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js.git"
  },
  "scripts": {
    "test": "node --unhandled-rejections=strict dist/test/e2e.js",
    "lint": "eslint --ext .js,.ts src/**",
    "prepare": "husky install"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.231.0",
    "@aws-sdk/client-lambda": "3.231.0",
    "@aws-sdk/client-s3": "3.231.0",
    "ansi-escapes": "4.3.2",
    "aws-lambda": "1.0.7",
    "chalk": "4.1.2",
    "dependency-tree": "8.1.2",
    "glob": "8.0.3",
    "table": "6.8.1",
    "tmp": "0.2.1",
    "ts-loader": "9.4.2",
    "uuid": "9.0.0",
    "webpack": "5.75.0",
    "yazl": "2.5.1"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.157",
    "@nordicsemiconductor/cloudformation-helpers": "7.0.7",
    "@types/aws-lambda": "8.10.109",
    "@types/dependency-tree": "8.1.1",
    "@types/glob": "8.0.0",
    "@types/node": "18.11.15",
    "@types/table": "6.3.2",
    "@types/terminal-kit": "2.5.1",
    "@types/tmp": "0.2.3",
    "@types/uuid": "9.0.0",
    "@types/webpack": "5.28.0",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.54.0",
    "cdk": "2.54.0"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  }
}
