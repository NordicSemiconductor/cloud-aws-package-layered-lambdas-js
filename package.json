{
  "name": "@nordicsemiconductor/package-layered-lambdas",
  "version": "0.0.0-development",
  "description": "Packages lambda functions and layers for AWS lambda",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js.git"
  },
  "scripts": {
    "test": "node --unhandled-rejections=strict dist/test/e2e.js",
    "lint": "eslint --ext .js,.ts src/**",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.8.0",
    "@aws-sdk/client-lambda": "3.8.0",
    "@aws-sdk/client-s3": "3.8.0",
    "ansi-escapes": "4.3.1",
    "aws-lambda": "1.0.6",
    "chalk": "4.1.0",
    "dependency-tree": "8.0.0",
    "glob": "7.1.6",
    "snyk": "1.483.0",
    "table": "6.0.7",
    "tmp": "0.2.1",
    "ts-loader": "8.0.18",
    "uuid": "8.3.2",
    "webpack": "5.24.4",
    "webpack-node-externals": "2.5.2",
    "yazl": "2.5.1"
  },
  "devDependencies": {
    "@aws-cdk/aws-lambda": "1.92.0",
    "@aws-cdk/aws-s3": "1.92.0",
    "@aws-cdk/core": "1.92.0",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "12.0.1",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "9.0.2",
    "@nordicsemiconductor/cloudformation-helpers": "6.0.9",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "7.0.11",
    "@types/aws-lambda": "8.10.72",
    "@types/dependency-tree": "8.0.0",
    "@types/glob": "7.1.3",
    "@types/node": "14.14.33",
    "@types/table": "6.0.0",
    "@types/terminal-kit": "1.28.2",
    "@types/tmp": "0.2.0",
    "@types/uuid": "8.3.0",
    "@types/webpack": "4.41.26",
    "@types/webpack-node-externals": "2.5.0",
    "@types/yazl": "2.4.2",
    "cdk": "1.92.0",
    "eslint": "7.21.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "pinst": "2.1.6",
    "prettier": "2.2.1",
    "typescript": "4.2.3"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "snyk": true
}
