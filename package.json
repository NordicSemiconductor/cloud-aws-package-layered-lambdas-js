{
  "name": "@nordicsemiconductor/package-layered-lambdas",
  "version": "0.0.0-development",
  "description": "Packages lambda functions and layers for AWS lambda",
  "type": "module",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js.git"
  },
  "scripts": {
    "test": "node --unhandled-rejections=strict dist/test/e2e.js",
    "lint": "eslint --ext .js,.ts src/**",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "keywords": [
    "aws",
    "lambda",
    "serverless"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-aws-package-layered-lambdas-js#readme",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.45.0",
    "@aws-sdk/client-lambda": "3.45.0",
    "@aws-sdk/client-s3": "3.45.0",
    "ansi-escapes": "5.0.0",
    "aws-lambda": "1.0.7",
    "chalk": "5.0.0",
    "dependency-tree": "8.1.2",
    "esbuild": "0.14.10",
    "glob": "7.2.0",
    "table": "6.8.0",
    "tmp": "0.2.1",
    "uuid": "8.3.2",
    "yazl": "2.5.1"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "10.0.7",
    "@nordicsemiconductor/cloudformation-helpers": "6.1.25",
    "@types/aws-lambda": "8.10.89",
    "@types/dependency-tree": "8.1.1",
    "@types/glob": "7.2.0",
    "@types/node": "17.0.8",
    "@types/table": "6.3.2",
    "@types/terminal-kit": "1.28.4",
    "@types/tmp": "0.2.3",
    "@types/uuid": "8.3.4",
    "@types/yazl": "2.4.2",
    "aws-cdk-lib": "2.4.0",
    "cdk": "2.4.0"
  },
  "files": [
    "package-lock.json",
    "dist/src",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga",
      {
        "name": "!(saga)",
        "prerelease": true
      }
    ],
    "remoteTags": true
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$"
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc"
}
